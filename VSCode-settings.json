{
  "security.workspace.trust.untrustedFiles": "open",
  "azureResourceGroups.groupBy": "resourceGroup",
  "editor.fontFamily": "JetBrainsMono NF",
  "editor.fontSize": 13.0,
  "editor.lineHeight": 1.7,
  "editor.fontLigatures": true,
  "editor.semanticHighlighting.enabled": true,
  "terminal.integrated.fontFamily": "JetBrainsMono NF",
  "terminal.integrated.shellIntegration.enabled": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".env": ".env, .*.env, .env.*",
    "appsettings.json": "appsettings.json, appsettings.*.json",
    "*.sln": "$(capture).sln, $(capture).sln.DotSettings",
    "*.csproj": "*.csproj, nuget.config",
    "*.razor": "$(capture).razor.css, $(capture).razor.cs",
    "*.cshtml": "$(capture).cshtml.cs"
  },
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.colorCustomizations": {
    "[Visual Studio Dark]": {
      "activityBar.background": "#292929",
      "activityBar.activeBackground": "#181818",
      "activityBar.activeBorder": "#FFFFFF",
      "activityBarBadge.background": "#CBCFD1",
      "activityBarBadge.foreground": "#181818",
      "editor.background": "#181818",
      "editor.foreground": "#C8C8C8",
      "editor.lineHighlightBackground": "#1D2746",
      "editor.selectionBackground": "#1B4A87",
      "editorGroupHeader.tabsBackground": "#292929",
      "tab.activeBorderTop": "#FFFFFF",
      "menu.background": "#292929",
      "menu.foreground": "#D1D0CD",
      "menu.selectionBackground": "#194176",
      "sideBar.background": "#181818",
      "sideBarSectionHeader.background": "#292929",
      "statusBar.background": "#181818",
      "titleBar.activeBackground": "#292929",
      "titleBar.inactiveBackground": "#292929",
      "titleBar.activeForeground": "#A1A1A1",
    }
  },
  "editor.tokenColorCustomizations": {
    "[Visual Studio Dark]": {
      "comments": "#808080",
      "strings": "#D27D80",
      "functions": "#E5C07B",
      "keywords": "#61AFEF",
      "variables": "#DCDCDC",
      "types": "#98C379",
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "foreground": "#808080",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "string.quoted.double", 
          "settings": {
            "foreground": "#D27D80"
          }
        },
        {
          "scope": "entity.name.function", 
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": "variable.other.object.property", 
          "settings": {
            "foreground": "#EDD2A0"
          }
        },
        {
          "scope":[
            "entity.name.variable.local"
          ],
          "settings": {
            "foreground": "#C8C8C8"
          }
        },
        {
          "scope":[
            "entity.name.variable.parameter"
          ],
          "settings": {
            "foreground": "#DCDCDC"
          }
        },
        {
          "scope":[
            "entity.name.variable.field"
          ],
          "settings": {
            "foreground": "#D2D2D2"
          }
        },
        {
          "scope": "entity.name.type.namespace", 
          "settings": {
            "foreground": "#CAE0BA"
          }
        },
        {
          "scope": "entity.name.class, entity.name.type", 
          "settings": {
            "foreground": "#98C379"
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Visual Studio Dark]": {
      "rules": {
        "comment": "#808080",
        "string": "#D27D80",
        "stringVerbatim": "#D27D80",
        "function": "#E5C07B",
        "property": "#EDD2A0",
        "keyword": "#61AFEF",
        "constant": "#B5DAF9",
        "field": {
          "foreground": "#D2D2D2",
          "fontStyle": "bold"
        },
        "variable": "#DCDCDC",
        "parameter": {
          "foreground": "#C8C8C8",
          "fontStyle": "italic"
        },
        "namespace": "#CAE0BA",
        "enum": "#98C379",
        "interface": "#98C379",
        "struct": "#98C379",
        "type": "#98C379",
        "class": "#98C379",
        "class.readonly": {
          "foreground": "#98C379",
          "fontStyle": "bold italic"
        },
        "*.declaration" : {
          "fontStyle": "bold"
        },
        "*.readonly" : "#DCDCDC"
      }
    }
  }
}